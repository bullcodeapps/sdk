{
  "root": true,
  "extends": [
    "@react-native-community"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": [ 
    "react-hooks"
  ],
  "rules": {
    "no-console": ["error", { "allow": ["tron", "log"] }],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "radix": "off"
  },
  "settings": {
    "import/resolver": "babel-plugin-root-import"
  }
}
